{
    "name": "prosemirror-findchanges",
    "version": "0.1.0",
    "description": "Find changes between two prosemirror documents without access to the steps.",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run build_js && npm run build_css",
        "build_js": "rollup demo/index.js -c -f iife -o demo/index-build.js",
        "build_css": "cat node_modules/prosemirror-example-setup/style/style.css node_modules/prosemirror-gapcursor/style/gapcursor.css node_modules/prosemirror-menu/style/menu.css > build/style.css"
    },
    "keywords": [
        "prosemirror",
        "change",
        "tracking"
    ],
    "author": "Johannes Wilm",
    "license": "???",
    "dependencies": {
        "diff-match-patch": "^1.0.1",
        "prosemirror-changeset": "^1.0.3",
        "prosemirror-example-setup": "^1.0.1",
        "prosemirror-model": "^1.5.0",
        "prosemirror-schema-basic": "^1.0.0",
        "prosemirror-schema-list": "^1.0.1",
        "prosemirror-state": "^1.2.0",
        "prosemirror-transform": "^1.0.10",
        "prosemirror-view": "^1.3.0",
        "rfc6902": "^2.2.2"
    },
    "devDependencies": {
        "rollup": "^0.59.4",
        "rollup-plugin-commonjs": "^9.1.3",
        "rollup-plugin-node-resolve": "^3.3.0"
    }
}
